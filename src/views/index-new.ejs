        videoElement.src = URL.createObjectURL(videoFile);
        videoElement.addEventListener("loadedmetadata", function () {
          const videoDuration = videoElement.duration;
          const formattedDuration = formatVideoDuration(videoDuration);
          var a = formattedDuration.split(':');
          var seconds = (+a[0]) * 60 * 60 + (+a[1]) * 60 + (+a[2]);
          if (seconds > 40) {
            $("#video_error").empty();
            $("#video_error").text("The video length should not be greater than 40 seconds");
            document.getElementById("video_error").style.display ="block";
            error_count =error_count+1;
            $('#videoreel').val("")

          } else {
            document.getElementById("video_error").style.display ="none";
          }
        });

      } else {
      }
    }

    function formatVideoDuration(duration) {
      const hours = Math.floor(duration / 3600);
      const minutes = Math.floor((duration % 3600) / 60);
      const seconds = Math.floor(duration % 60);
      return `${hours}:${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;
    }


