  <script>
    function nameValidater(evt) {

      var inputValue = (evt.which) ? evt.which : evt.keyCode

      if (!(inputValue >= 65 && inputValue <= 93) && (inputValue <= 95 && inputValue <= 120) && (inputValue != 32 && inputValue != 0))

        return false;

      return true;

    }

    function onlyNumberKey(evt) {

      var ASCIICode = (evt.which) ? evt.which : evt.keyCode

      if (ASCIICode > 31 && (ASCIICode < 48 || ASCIICode > 57))

        return false;

      return true;

    }

    function emailvalidation() {
      var email = $('#email').val();
      // alert("test")
      var reg = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
      if(email.length ==0){
        document.getElementById("email_error").style.display ="none";

      }else
      if (!reg.test(email)) {
        $("#email_error").empty();
        $("#email_error").text("Please Enter Valid Email");
        document.getElementById("email_error").style.display ="block";
        // alert("Please enter valid email");
        // $('#email').val("")
	    // return  false

      }else{
        document.getElementById("email_error").style.display ="none";

      }
    }

    function emailvalidationreel() {
      var email = $('#emailreel').val();

      var reg = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
      if(email.length ==0){
        document.getElementById("emailreel_error").style.display ="none";

      }else
      if (!reg.test(email)) {
        $("#emailreel_error").empty();
        $("#emailreel_error").text("Please Enter Valid Email");
        document.getElementById("emailreel_error").style.display ="block";
        error_count =error_count+1;
        // alert("Please enter valid email");
        $('#emailreel').val("")
        return false
      }else{
        document.getElementById("emailreel_error").style.display ="none";

      }
    }

    function ageValidation() {
      var age = $('#age').val();
      var reg = /^[0-9]+$/;
      if (!reg.test(age)) {

        $("#age_error").empty();
        $("#age_error").text("Age should be numbers only");
        document.getElementById("age_error").style.display ="block";
        error_count =error_count+1;

      } else if ((age.length) >= 3 || (age == 0)) {
        $("#age_error").empty();
        $("#age_error").text("Please enter a valid age");
        document.getElementById("age_error").style.display ="block";
        error_count =error_count+1;
        
      }else{
        document.getElementById("age_error").style.display ="none";
      }
    }

    function ageValidationreel() {
      var age = $('#agereel').val();
      if (age >= 120) {
        $("#agereel_error").empty();
        $("#agereel_error").text("Enter a Valid Age");
        document.getElementById("agereel_error").style.display ="block";
        error_count =error_count+1;
      } else if ((age.length) >= 3 || (age == 0)) {
        $("#agereel_error").empty();
        $("#agereel_error").text("Enter a Valid Age");
        document.getElementById("agereel_error").style.display ="block";
        error_count =error_count+1;
      }else{
        document.getElementById("agereel_error").style.display ="none";

      }
    }

    function phonevalidation() {

      var mobile = $('#mobile').val();
      var reg = /^[0-9]+$/;
      if (mobile.trim() == '') {
        $("#phone_error").empty();
        $("#phone_error").text("Mobile number required!");
        document.getElementById("phone_error").style.display ="block";
	      error_count =error_count+1;
      } else if (!reg.test(mobile)) {
        $("#phone_error").empty();
        $("#phone_error").text("Mobile number should be numbers only");
        document.getElementById("phone_error").style.display ="block";
	      error_count =error_count+1;
      } else if ((mobile.length) < 10 || (mobile.length) > 10) {
        $("#phone_error").empty();
        $("#phone_error").text("Mobile should only be 10 digits");
        document.getElementById("phone_error").style.display ="block";
	      error_count =error_count+1;
      }else{
        document.getElementById("phone_error").style.display ="none";
